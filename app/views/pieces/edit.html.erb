<%= form_for @parte do |p|%>
  <div id="formulario">
    <table>
      <tr>
        <td><%= label_tag nil, "Nombre de la parte:" %></td>
        <td><%= p.text_field :part_nombre %></td>
      </tr>
      <tr>
        <td colspan="2" class="boton"><%= p.submit "Guardar"%></td>
      </tr>
    </table>
  </div>
<%end%>