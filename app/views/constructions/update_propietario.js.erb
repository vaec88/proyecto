  $('#pers_propiet_nombres').val("<%= @propietario.pers_nombre%>");
  $('#pers_propiet_direccion').val("<%= @propietario.pers_direccion%>");
  $('#pers_propiet_telefono1').val("<%= @propietario.pers_telefono1%>");
  $('#pers_propiet_telefono2').val("<%= @propietario.pers_telefono2%>");
